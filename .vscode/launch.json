{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "FastAPI - Debug with Uvicorn",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "backend.main:app",         // Ersetze "main:app" ggf. durch deinen Importpfad
                "--reload",
                "--host=0.0.0.0",
                "--port=8000"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
